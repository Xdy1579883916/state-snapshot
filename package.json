{
  "name": "state-snapshot",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.4.0",
  "description": "只需将你的状态推送到 state-snapshot 中，即可实现 undo / redo 操作！",
  "author": "XiaDeYu <1579883916@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/Xdy1579883916/state-snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xdy1579883916/state-snapshot.git"
  },
  "bugs": "https://github.com/Xdy1579883916/state-snapshot/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "flatted": "^3.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.5.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^0.7.10",
    "@types/node": "latest",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.8.2",
    "unbuild": "^2.0.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "bundledDependencies": [
    "flatted"
  ]
}
